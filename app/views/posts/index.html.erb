<% @posts.each do |post|%>
    <p><h1><%= post.user.username%></h1><%= post.body%></p>
    <% if post.images.attached?%>
        <% post.images.each do |image|%>
            <p><%= image_tag image, width: "200px"%></p>
        <%end%>
    <%end%>
    <div>
        <%= link_to ' Edit', edit_post_path(post)%>
    </div>
<%end%>